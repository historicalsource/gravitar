NWVYGE:	LDA I,0
	LDX I,0D
	BEGIN
	STA X,LACPVF		;0 VELOCITIES + POSITIONS
	DEX
	MIEND
	LDX I,0A7
	BEGIN
	STA AX,BACPVH
	DEX
	EQEND
	JSR CLRSHT
	LDA I,4
	STA QUADRANT
	JSR INISOU
	LDA I,20
	STA LANGLE
	STA SWCNT
	STA ASDETM+2
	STA ASDETM+3
	LDX PLAYUP
	LDA X,CRPLNT
	TAX
	LDA X,INLAVP
	STA LACPVH
	LDA ATRAFL
	IFNE
	LDA X,PLANID
	CMP I,0A8
	IFNE
	JSR SETLVL
	ELSE
	JSR REINSN
	THEN
	THEN
	JSR INPNTR
	LDX PLAYUP
	LDY X,WAVENO
	LDA OPTSW1
	AND I,10
	IFEQ
	LDA Y,GRAFCT
	ELSE
	LDA Y,EGRAVY
	THEN
	STA GRAVTY		;INITIALIZE GRAVITY
	LDA Y,BPATSP
	STA X,PATSPD
	LDA I,10
	STA PMXVEL
	LDA I,-10
	STA NMXVEL
	STA X,PLADED
	LDA Y,BASPRO
	STA X,SHOTPR
	LDA I,0
	STA CNTRVH
	STA CNTRVL
	STA CNTRHH
