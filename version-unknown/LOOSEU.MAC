	LDA I,2
	STA X,ASHSPD
	LDA I,1
	STA X,FUEL3
	LDA I,CNEWGA
	STA X,STATE
	LDA I,CDSETU
	STA X,DSTATE
	DEX
	MIEND
	STX PLADED
	STX PLADED+1
	JSR SETXR
	LDX I,1
	LDA I,0
