	.BYTE 10,28,28,38,8,18,10
LPLCOR: LDA X,PLCPVL
	STA POINT1
	LDA X,PLCPVH
	STA POINT2
	LDA X,PLCPHL
	STA POINT3
	LDA X,PLCPHH
	STA POINT4
	RTS
SAVBAS: LDY I,7
	LDX PLAYUP
	LDA X,CRPLNT
	ASL
	CLC
	ADC PLAYUP
	TAX
	BEGIN
	LDA Y,BSTAT
	CLC
	IFNE
	SEC
	THEN
	ROL X,HBSTAT
	DEY
	MIEND
	RTS

SAVFUE: LDY I,7
	LDX PLAYUP
	LDA X,CRPLNT
	ASL
	CLC
	ADC PLAYUP
	TAX
	BEGIN
	LDA Y,FSTAT+8
	CLC
	IFNE
	SEC
	THEN
	ROR X,LFSTAT
	LDA Y,FSTAT
	CLC
	IFNE
	SEC
	THEN
	ROR X,HFSTAT
	DEY
	MIEND
	RTS

	.PAGE
